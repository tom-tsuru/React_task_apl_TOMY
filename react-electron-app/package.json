{
  "name": "react-electron-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run revert-mjs && tsc -b --clean && tsc -b && npm run rename-mjs && vite build",
    "revert-mjs": "powershell -Command \"Get-ChildItem -Recurse -Filter 'preload.mjs' ./dist/main | Remove-Item\"",
    "rename-mjs": "powershell -Command \"Get-ChildItem -Recurse -Filter 'preload.js' ./dist/main | Rename-Item -NewName { $_.Name -replace '\\.js$', '.mjs' }\"",
    "electron": "electron .",
    "start": "concurrently \"npm run dev\" \"npm run electron .\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/x-date-pickers": "^7.23.3",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.8.1",
    "dayjs": "^1.11.13",
    "electron": "^32.0.0",
    "express": "^4.21.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^5.0.0",
    "vite-plugin-html": "^3.2.2",
    "wait-on": "^8.0.2"
  }
}
